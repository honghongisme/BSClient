<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.picturerecognize">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".module.personal.upload.HeadImageUploadActivity"></activity>
        <activity android:name=".module.social.user.UserActivity" />
        <activity android:name=".module.social.commit.CommitActivity" />
        <activity android:name=".module.social.publish.PostActivity" />
        <activity android:name=".module.chart.payment.ClassifyPaymentActivity" />
        <activity android:name=".module.budget.add.AddBudgetActivity" />
        <activity android:name=".module.budget.BudgetActivity" />
        <activity android:name=".module.login.LoginActivity" />
        <activity android:name=".module.chart.ChartActivity"></activity>
        <activity android:name=".module.bill.BillActivity" />
        <activity android:name=".module.detail.DetailActivity" />
        <activity android:name=".BaseActivity" />
        <activity android:name=".module.add.AddActivity" />
        <activity
            android:name=".module.MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>